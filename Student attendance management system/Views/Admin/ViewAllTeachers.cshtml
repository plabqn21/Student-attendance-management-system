@model IEnumerable<Student_attendance_management_system.Models.ApplicationUser>

@{
    ViewBag.Title = "ViewAllTeacherss";
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

<h2>All Teachers</h2>

<div style="margin-left: 140px">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>

            <th>
                @Html.DisplayNameFor(model => model.ProfilePicture)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateOfBirth)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Degree)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Position)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Mobile)
            </th>


            <th>
                @Html.DisplayNameFor(model => model.Email)
            </th>



            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    <img width="70" height="70" src="@Url.Content(item.ProfilePicture)" />
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateOfBirth)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Degree)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Position)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Mobile)
                </td>


                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>



            </tr>
        }

    </table>
</div>